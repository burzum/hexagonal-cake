{
    "name": "burzum/hexagonal-cake",
    "description": "Hexagonal CakePHP Application Skeleton",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "cakephp/cakephp": "^3.6",
        "cakephp/plugin-installer": "^1.1",
        "league/container": "^2.4",
        "league/tactician": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "cakephp/debug_kit": "^3.15",
        "squizlabs/php_codesniffer": "*",
        "twig/twig": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Kr√§mer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\PHPStan\\": "src",
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "config": {
        "bin-dir": "bin",
        "prestissimo": {
            "maxConnections": 12,
            "minConnections": 6
        }
    }
}
